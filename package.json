{
  "name": "gt-librarian",
  "version": "1.2.1",
  "description": "AI-powered chat embed block for WordPress",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:widget && npm run build:wp",
    "build:widget": "webpack --config webpack.widget.config.js",
    "build:wp": "wp-scripts build",
    "start": "wp-scripts start",
    "dev:widget": "webpack --config webpack.widget.config.js --watch",
    "dev": "concurrently \"npm run dev:widget\" \"npm run start\""
  },
  "keywords": [
    "wordpress",
    "gutenberg",
    "block",
    "chat",
    "ai"
  ],
  "author": "",
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "deep-chat": "^1.4.11",
    "remarkable": "^2.0.1"
  },
  "devDependencies": {
    "@wordpress/scripts": "^27.0.0",
    "@wordpress/block-editor": "^13.0.0",
    "@wordpress/blocks": "^13.0.0",
    "@wordpress/components": "^28.0.0",
    "@wordpress/element": "^6.0.0",
    "@wordpress/i18n": "^5.0.0",
    "@wordpress/api-fetch": "^7.0.0",
    "@wordpress/server-side-render": "^5.0.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.4.5",
    "ts-loader": "^9.5.0",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  }
}
